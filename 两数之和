class Solution
{
    public int[] twoSum(int[] nums, int target) 
    {
        int l=nums.length;
        for(int i =0;i<l;i++)
        {
            for(int j=i+1;j<l;j++)
            {
                if(target==nums[i]+nums[j])
                {
                    return new int[]{i,j};
                }
            }
        }
        return new int[0];
    }
}
